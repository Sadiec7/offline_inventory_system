{
  "name": "offline_inventory_system",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . CRONOS-Inventario --platform=win32 --arch=x64 --out=dist/ --overwrite --icon=assets/icon.ico",
    "build-all": "electron-packager . CRONOS-Inventario --platform=all --arch=x64 --out=dist/ --overwrite",
    "build-win": "electron-packager . CRONOS-Inventario --platform=win32 --arch=x64 --out=dist/ --overwrite --icon=assets/cronos-logo.ico",
    "build-mac": "electron-packager . CRONOS-Inventario --platform=darwin --arch=x64 --out=dist/ --overwrite --icon=assets/icon.icns",
    "build-linux": "electron-packager . CRONOS-Inventario --platform=linux --arch=x64 --out=dist/ --overwrite --icon=assets/icon.png"
  },
  "dependencies": {
    "@electron/remote": "^2.1.3",
    "electron": "^28.1.0",
    "exceljs": "^4.4.0",
    "sqlite3": "^5.1.6",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "electron-packager": "^17.1.2"
  }
}